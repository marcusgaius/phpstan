{
	"name": "marcusgaius/phpstan",
	"description": "PHPStan configuration for Craft CMS projects",
	"version": "1.2.0",
	"require": {
		"phpstan/phpstan": "^2.0",
		"phpstan/phpstan-strict-rules": "^2.0"
	},
	"autoload": {
		"psr-4": {
			"MarcusGaius\\PHPStan\\": "src/"
		}

	},
	"require-dev": {
		"craftcms/cms": "^4.14"
	},
	"config": {
		"allow-plugins": {
			"yiisoft/yii2-composer": true,
			"craftcms/plugin-installer": true
		}
	}
}
